# RUN: comet-opt --convert-ta-to-it --convert-to-loops --convert-to-llvm %s &> mult_dense_matrix.llvm
# RUN: mlir-cpu-runner mult_dense_matrix.llvm -O3 -e main -entry-point-result=void -shared-libs=%comet_utility_library_dir/libcomet_runner_utils%shlibext,%mlir_utility_library_dir/libomp%shlibext | FileCheck %s


def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [8];
	IndexLabel [j] = [4];  
	IndexLabel [k] = [2];           

	#Tensor Declarations
	Tensor<double> A([i, j], {Dense});	  
	Tensor<double> B([j, k], {Dense});
	Tensor<double> C([i, k], {Dense});

	#Tensor Fill Operation
	A[i, j] = 2.2;
	B[j, k] = 3.4;
	C[i, k] = 0.0;

	C[i, k] = A[i, j] * B[j, k];
	print(C);
}

