def main() {
	#IndexLabel Declarations
	IndexLabel [a] = [?];
	IndexLabel [b] = [?];
	IndexLabel [c] = [?];
    IndexLabel [d] = [?];
	IndexLabel [e] = [?];
	IndexLabel [f] = [?];

	#Tensor Declarations
	Tensor<double> A([a, b], {CSR});	  
    Tensor<double> B([c, d], {CSR});
	Tensor<double> C([e, f], {CSR});

	#Tensor Fill Operation
	A[a, b] = comet_read(0);
    B[c, d] = comet_read(1);
	C[e, f] = comet_read(2);

	print(A);
    print(B);
	print(C);
}

