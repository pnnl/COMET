# Dense vector sparse matrix multiplication
# Sparse matrix is in DCSR format
# RUN: comet-opt --convert-ta-to-it --convert-to-loops %s &> mult_DenseVecxDCSR.mlir
# RUN: mlir-opt --convert-scf-to-std --convert-std-to-llvm mult_DenseVecxDCSR.mlir &> mult_DenseVecxDCSR.llvm
# RUN: export SPARSE_FILE_NAME0=%comet_integration_test_data_dir/test_rank2.mtx
# RUN: mlir-cpu-runner mult_DenseVecxDCSR.llvm -O3 -e main -entry-point-result=void -shared-libs=%mlir_utility_library_dir/libmlir_runner_utils%shlibext,%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s


def main() {
	#IndexLabel Declarations
	IndexLabel [a] = [?];
	IndexLabel [b] = [?];           

	#Tensor Declarations
	Tensor<double> B([a,b], {DCSR});
	Tensor<double> A([a], {Dense});	  
	Tensor<double> C([b], {Dense});

	#Tensor Fill Operation
	A[a] = 1.7;
	B[a, b] = comet_read(0);
	C[b] = 0.0;

	C[b] = A[a] * B[a, b];
	print(C);
}

