# RUN: export SPARSE_FILE_NAME0=%comet_integration_test_data_dir/test_rank2.mtx
# RUN: mlir-cpu-runner eltwise_DCSRxDense_oDense.llvm -O3 -e main -entry-point-result=void -shared-libs=%mlir_utility_library_dir/libmlir_runner_utils%shlibext,%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s

def main() {
	#IndexLabel Declarations
	IndexLabel [a] = [?];
	IndexLabel [b] = [?];            

	#Tensor Declarations
	Tensor<double> A([a, b], {DCSR});	  
	Tensor<double> B([a, b], {Dense});
	Tensor<double> C([a, b], {Dense});

    #Tensor Readfile Operation
    A[a, b] = comet_read(0);
	B[a, b] = 2.7;

	#Initialize output dense tensor
	C[a, b] = 0.0;

	#Tensor Contraction
	C[a, b] = A[a, b] .* B[a, b];
	
	print(C);
}

