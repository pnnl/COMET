# RUN: mlir-opt --convert-linalg-to-loops --convert-scf-to-std --convert-std-to-llvm Dense_mult_dTranspose.mlir &> Dense_mult_dTranspose.llvm
# RUN: mlir-cpu-runner Dense_mult_dTranspose.llvm -O3 -e main -entry-point-result=void -shared-libs=%mlir_utility_library_dir/libmlir_runner_utils%shlibext,%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s

def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [5];
	IndexLabel [j] = [5];      
    IndexLabel [k] = [5];     

	#Tensor Declarations
	Tensor<double> B([i, j], Dense);
    Tensor<double> A([k, j], Dense);	  
    Tensor<double> C([k, i], Dense);

    #Tensor Readfile Operation      
    B[i, j] = 3.2;
    A[k, j] = 2.3;
    C[k, i] = 0.0;

	#Tensor Transpose
	C[k, i] =  A[k, j] * transpose(B[i, j],{j,i});
	print(C);
}
