def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [?];
	IndexLabel [j] = [?];      
    IndexLabel [k] = [?];     

	#Tensor Declarations
    Tensor<double> A([k, j], CSR);	  
    Tensor<double> B([i, j], CSR);
    Tensor<double> C([k, i], CSR);

    #Tensor Readfile Operation      
    A[k, j] = comet_read(0);
    B[i, j] = comet_read(0);

	#Tensor Transpose
	C[k, i] = A[k, j] * transpose(B[i, j],{j,i});
	print(C);
}
