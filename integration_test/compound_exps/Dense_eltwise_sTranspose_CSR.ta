def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [?];
	IndexLabel [j] = [?];                   

	#Tensor Declarations
	Tensor<double> B([i, j], CSR);
	Tensor<double> C([j, i], CSR);
	Tensor<double> A([j, i], Dense);	  

    #Tensor Readfile Operation      
    B[i, j] = comet_read(0);
	A[j, i] = 3.2;
	
	#Tensor Transpose
	C[j, i] = A[j, i] .* transpose(B[i, j],{j,i});
	print(C);
}

