    IndexLabel [k] = [5];     

	#Tensor Declarations
	Tensor<double> B([i, j], CSR);
    Tensor<double> A([k, j], Dense);	  
    Tensor<double> C([k, i], Dense);

    #Tensor Readfile Operation      
    B[i, j] = comet_read(0);
    A[k, j] = 2.3;
    C[k, i] = 0.0;

	#Tensor Transpose
	C[k, i] =  A[k, j] * transpose(B[i, j],{j,i});
	print(C);
}
