# RUN: comet-opt --opt-multiop-factorize --convert-ta-to-it --convert-to-loops --convert-to-llvm %s &> chain_mult_factorize.llvm
# RUN: mlir-cpu-runner chain_mult_factorize.llvm -O3 -e main -entry-point-result=void -shared-libs=%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s


def main() {
	#IndexLabel Declarations
	IndexLabel [i] = [2];
	IndexLabel [j] = [2];  
	IndexLabel [k] = [5];           
	IndexLabel [l] = [2];           

	#Tensor Declarations
	Tensor<double> A([i, j], {Dense});	  
	Tensor<double> B([j, k], {Dense});
	Tensor<double> C([k, l], {Dense});
	Tensor<double> D([i, l], {Dense});

	#Tensor Fill Operation
	A[i, j] = 2.2;
	B[j, k] = 3.4;
	C[k, l] = 1.0;
	D[i, l] = 0.0;

	D[i, l] = A[i, j] * B[j, k] * C[k,l];
	print(D);
}

