# Dense matrix - sparse matrix  multiplication
# Sparse matrix is in COO format
# RUN: comet-opt --convert-ta-to-it --convert-to-loops --convert-to-llvm %s &> mm_SemiringPlusTimes_DensexCOO_oDense.llvm
# RUN: export SPARSE_FILE_NAME0=%comet_integration_test_data_dir/test_rank2.mtx
# RUN: mlir-cpu-runner mm_SemiringPlusTimes_DensexCOO_oDense.llvm -O3 -e main -entry-point-result=void -shared-libs=%mlir_utility_library_dir/libmlir_runner_utils%shlibext,%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s

def main() {
	#IndexLabel Declarations
	IndexLabel [a] = [4];
	IndexLabel [b] = [?];
	IndexLabel [c] = [?];             

	#Tensor Declarations
	Tensor<double> B([b, c], {COO});   #sparse tensor declarations should be before dense tensor declarations
	Tensor<double> A([a, b], {Dense});	  
	Tensor<double> C([a, c], {Dense});

	#Tensor Fill Operation
	A[a, b] = 1.7;
	B[b, c] = comet_read(0);
	C[a, c] = 0.0;

	C[a, c] = A[a, b] @(+,*) B[b, c];
	print(C);
}

