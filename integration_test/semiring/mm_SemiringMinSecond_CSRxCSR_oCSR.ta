def main() {
    #IndexLabel Declarations
    IndexLabel [a] = [?];
    IndexLabel [b] = [?];
    IndexLabel [c] = [?];
    
    #Tensor Declarations
    Tensor<double> A([a, b], {CSR});	 
    Tensor<double> B([b, c], {CSR});
    Tensor<double> C([a, c], {CSR});
    
    #Tensor Readfile Operation
    A[a, b] = comet_read(0);
    B[b, c] = comet_read(1);
    
    #Tensor Contraction
    C[a, c] = A[a, b] @(min,second) B[b, c];
    print(C);
}

