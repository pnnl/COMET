# RUN: comet-opt --convert-ta-to-it --convert-to-loops %s &> eltwise_monoidTimes_dense_4Dtensors.mlir
# RUN: mlir-opt --convert-scf-to-std --convert-std-to-llvm eltwise_monoidTimes_dense_4Dtensors.mlir &> eltwise_monoidTimes_dense_4Dtensors.llvm
# RUN: mlir-cpu-runner eltwise_monoidTimes_dense_4Dtensors.llvm -O3 -e main -entry-point-result=void -shared-libs=%mlir_utility_library_dir/libmlir_runner_utils%shlibext,%comet_utility_library_dir/libcomet_runner_utils%shlibext | FileCheck %s

def main() {
	# IndexLabel Declarations
	IndexLabel [a] = [2];
	IndexLabel [b] = [2];
	IndexLabel [c] = [2];
	IndexLabel [d] = [2];

	# Tensor Declarations
	Tensor<double> A([a, b, c, d], {Dense});
	Tensor<double> B([a, b, c, d], {Dense});
	Tensor<double> C([a, b, c, d], {Dense});

	# Tensor Fill Operation
	A[a, b, c, d] = 2.2;
	B[a, b, c, d] = 3.6;
	C[a, b, c, d] = 0.0;

	# Tensor Contraction
	C[a, b, c, d] = A[a, b, c, d] @(*) B[a, b, c, d];
	
	print(C);
}

