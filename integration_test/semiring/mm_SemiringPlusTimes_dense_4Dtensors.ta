# This example comes from TCCG  -  ccsd12
def main() {
	# IndexLabel Declarations
	IndexLabel [a] = [2];
	IndexLabel [c] = [2];
	IndexLabel [b] = [2];
	IndexLabel [e] = [2];
	IndexLabel [d] = [2];
	IndexLabel [f] = [2];

	# Tensor Declarations
	Tensor<double> A([a, e, d, f], {Dense});
	Tensor<double> B([b, f, c, e], {Dense});
	Tensor<double> C([a, b, c, d], {Dense});

	# Tensor Fill Operation
	A[a, e, d, f] = 2.2;
	B[b, f, c, e] = 3.6;
	C[a, b, c, d] = 0.0;

	# Tensor Contraction
	C[a, b, c, d] = A[a, e, d, f] @(+,*) B[b, f, c, e];
	print(C);
}

